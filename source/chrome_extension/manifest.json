{
    "manifest_version": 2,
    "name": "Apaxy 2",
    "description": "Replace boring Apache/nginx directory listing with fancy directory listing with keyboard navigation",
    "homepage_url": "https://github.com/VarunAgw/Apaxy-2",
    "version": "4.1.0",
    "icons": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "96": "icons/icon96.png",
        "96": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["content_script.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "view/*"
    ]
}